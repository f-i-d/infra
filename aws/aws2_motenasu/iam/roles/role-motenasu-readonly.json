{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": "*",
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "aws:RequestedRegion": [
                        "ap-northeast-1"
                    ]
                }
            }
        },
        {
            "Effect": "Deny",
            "NotAction": [
                "iam:List*",
                "iam:Get*",
                "iam:Generate*",
                "ec2:Describe*",
                "s3:List*",
                "s3:Get*",
                "rds:Describe*",
                "lambda:ListFunctions",
                "lambda:GetFunction",
                "dynamodb:Describe*",
                "cloudfront:Get*",
                "states:List*",
                "states:Describe*",
                "backup:List*",
                "kms:Describe*",
                "kms:List*",
                "redshift:Describe*"
            ],
            "Resource": "*"
        }
    ]
}