{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Principal": {
				"AWS": "arn:aws:iam::368462884078:user/iam-op-test"
			},
			"Effect": "Allow",
			"Action": "sts:AssumeRole",
			"Condition": {
				"StringEquals": {
					"aws:RequestedRegion": [
						"ap-northeast-1"
					]
				}
			}
		},
		{
			"Effect": "Deny",
			"Principal": {
				"AWS": "arn:aws:iam::368462884078:user/iam-op-test"
			},
			"NotAction": [
				"iam:List*",
				"iam:Get*",
				"iam:Generate*",
				"ec2:Describe*",
				"s3:List*",
				"s3:Get*",
				"rds:Describe*",
				"lambda:ListFunctions",
				"lambda:GetFunction",
				"dynamodb:Describe*",
				"cloudfront:Get*",
				"states:List*",
				"states:Describe*",
				"backup:List*",
				"kms:Describe*",
				"kms:List*",
				"redshift:Describe*"
			]
		}
	]
}